<div class="container">
  <h3><%= link_to "Create a new article", new_article_path %></h3>
  <% @articles.each do |article| %>
  <div>
    <%= link_to article_path(article) do %>
    <h1><%=article.title%></h1>
    <% end %>
    <p><%=article.content%></p>
    <p> - <%= article.created_at.strftime("%d/%m/%Y") %></p>
  </div>
  <% end %>
</div>
